{
    "name": "shanbayTool",
    "manifest_version": 2,
    "version": "1.0",
    "description": "shanbayTool",
    "browser_action": {
        "default_icon": "img/icon.png",
        "default_popup": "popup.html"
    },
    "content_scripts": [{
        "matches": ["<all_urls>"],
        "js": ["js/index.js"]
    }]
}