{
    "name": "shanbayTool",
    "manifest_version": 2,
    "version": "1.0",
    "description": "shanbayTool",
    "icons": {
        "16": "img/icon16.png",
        "48": "img/icon48.png",
        "128": "img/icon128.png"
    },
    "browser_action": {
        "default_icon": "img/icon.png",
        "default_popup": "popup.html"
    },
    "content_scripts": [{
        "matches": ["<all_urls>"],
        "js": ["js/index.js"],
        "css": ["css/index.css"]
    }]
}